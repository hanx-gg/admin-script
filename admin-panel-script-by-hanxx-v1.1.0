-- Status Updater
local lastTick = tick()
local frameCount = 0

RunService.Heartbeat:Connect(function()
    -- Update FPS
    frameCount = frameCount + 1
    if tick() - lastTick >= 1 then
        local fps = math.floor(frameCount / (tick() - lastTick))
        frameCount = 0
        lastTick = tick()
        
        -- Color code FPS
        if fps < 20 then
            FPSLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            FPSLabel.Text = "FPS: "..fps.." (LOW)"
        elseif fps < 45 then
            FPSLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
            FPSLabel.Text = "FPS: "..fps.." (MED)"
        else
            FPSLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
            FPSLabel.Text = "FPS: "..fps.." (HIGH)"
        end
    end
    
    -- Update Player Position
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local pos = LocalPlayer.Character.HumanoidRootPart.Position
        PosLabel.Text = string.format("Position: (%.1f, %.1f, %.1f)", pos.X, pos.Y, pos.Z)
    end
    
    -- Update Player Count
    PlayerLabel.Text = "Players: "..#Players:GetPlayers()
    
    -- Update Ping (simulated)
    local ping = math.random(30, 200)
    if ping > 150 then
        PingLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
        PingLabel.Text = "Ping: "..ping.."ms (HIGH)"
    else
        PingLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        PingLabel.Text = "Ping: "..ping.."ms (GOOD)"
    end
end)

